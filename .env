# AWS Key and Secret are used to initialize aws cli within the container
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Default region is used both for the aws cli, and Cloudmapper config
AWS_DEFAULT_REGION=

# This is the AWS Account Name
ACCOUNT=

# External Port is used in the docker-compose.yml, change if you want to access
# this on something other than localhost:8000
EXTERNAL_PORT=8000

# true to enable Cloudmapper demo mode
DEMO_MODE=false

# cli options for prepare subcommand
PREPARE_OPTS=
